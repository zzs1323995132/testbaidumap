<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.FirstlyActivity">

    <Button
        android:id="@+id/btn_activity_welcome_init"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="初始化" />

    <Button
        android:id="@+id/btn_activity_welcome_overlay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="覆盖物" />

    <Button
        android:id="@+id/btn_activity_welcome_route"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="路线" />

    <Button
        android:id="@+id/btn_activity_welcome_poiqury"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Poi搜索" />

    <Button
        android:id="@+id/btn_activity_welcome_busline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="公交路线" />

    <Button
        android:id="@+id/btn_activity_welcome_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="定位" />

    <Button
        android:id="@+id/btn_activity_welcome_routemap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="路线查询" />
</LinearLayout>
